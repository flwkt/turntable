{
  "name": "@flwkt/turntable",
  "version": "0.2.4",
  "description": "Create a turntable view from your GLB/GLTF model",
  "type": "module",
  "license": "MIT",
  "main": "npm/turntable.js",
  "types": "npm/turntable.d.ts",
  "files": [
    "npm"
  ],
  "keywords": [
    "3d",
    "viewer",
    "turntable",
    "gltf",
    "glb",
    "flowkit",
    "flwkt"
  ],
  "scripts": {
    "build:npm": "tsc --project tsconfig.npm.json",
    "build:cjs": "node build.cjs",
    "build:all": "npm run build:npm && npm run build:cjs",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:npm": "npm run build:all && npm publish"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.1",
    "esbuild": "0.24.0",
    "rollup": "4.24.3",
    "typescript": "5.6.2",
    "vite": "5.4.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flwkt/turntable.git"
  }
}
